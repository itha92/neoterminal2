<div class="row">
  
<p id="notice"><%= notice %></p>

<p>
  <strong>Hora salida:</strong>
  <%= @itinerario.hora_salida.to_formatted_s(:time) %>
</p>

<p>
  <strong>Destino:</strong>
  <%= @itinerario.destino %>
</p>

<p>
  <strong>Origen:</strong>
  <%= @itinerario.origen %>
</p>

<p>
  <strong>Fecha:</strong>
  <%= @itinerario.fecha %>
</p>

<p>
  <strong>Terminal:</strong>
  <%= @itinerario.terminal.codigo %>
</p>

  <%= link_to 'Editar', edit_itinerario_path(@itinerario), class:"button" %>
  <%= link_to 'Atras', itinerarios_path, class:"button" %>
</div>
